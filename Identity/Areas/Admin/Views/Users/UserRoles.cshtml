@model IList<string>  
@{
    ViewData["Title"] = "UserRoles";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>User Roles</h1>
    
<a asp-action="Index">Roles Of : @ViewBag.UserInfo</a>
<table class="table">
    <thead>
            <tr>Role Name</tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @item
                </td>
            </tr>
        }
    </tbody>
</table>

